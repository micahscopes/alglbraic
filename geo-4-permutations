Geo inner(Geo a, Geo b){
return Geo(
	vec4(-a.q0[1]*b.q0[1] - a.q0[2]*b.q0[2] - a.q0[3]*b.q0[3] - a.q1[0]*b.q1[0] - a.q1[1]*b.q1[1] - a.q1[2]*b.q1[2] - a.q1[3]*b.q1[3] - a.q2[0]*b.q2[0] - a.q2[1]*b.q2[1] - a.q2[2]*b.q2[2] + a.q2[3]*b.q2[3] + a.q3[0]*b.q3[0] + a.q3[1]*b.q3[1] + a.q3[2]*b.q3[2] + a.q3[3]*b.q3[3],a.q0[2]*b.q1[1] + a.q0[3]*b.q1[2] + a.q1[0]*b.q1[3] - a.q1[1]*b.q0[2] - a.q1[2]*b.q0[3] - a.q1[3]*b.q1[0] - a.q2[0]*b.q2[3] - a.q2[1]*b.q3[0] - a.q2[2]*b.q3[1] - a.q2[3]*b.q2[0] - a.q3[0]*b.q2[1] - a.q3[1]*b.q2[2] - a.q3[2]*b.q3[3] + a.q3[3]*b.q3[2],-a.q0[1]*b.q1[1] + a.q0[3]*b.q2[0] + a.q1[0]*b.q2[1] + a.q1[1]*b.q0[1] + a.q1[2]*b.q2[3] + a.q1[3]*b.q3[0] - a.q2[0]*b.q0[3] - a.q2[1]*b.q1[0] - a.q2[2]*b.q3[2] + a.q2[3]*b.q1[2] + a.q3[0]*b.q1[3] + a.q3[1]*b.q3[3] - a.q3[2]*b.q2[2] - a.q3[3]*b.q3[1],-a.q0[1]*b.q1[2] - a.q0[2]*b.q2[0] + a.q1[0]*b.q2[2] - a.q1[1]*b.q2[3] + a.q1[2]*b.q0[1] + a.q1[3]*b.q3[1] + a.q2[0]*b.q0[2] + a.q2[1]*b.q3[2] - a.q2[2]*b.q1[0] - a.q2[3]*b.q1[1] - a.q3[0]*b.q3[3] + a.q3[1]*b.q1[3] + a.q3[2]*b.q2[1] + a.q3[3]*b.q3[0]),
	vec4(-a.q0[1]*b.q1[3] - a.q0[2]*b.q2[1] - a.q0[3]*b.q2[2] - a.q1[1]*b.q3[0] - a.q1[2]*b.q3[1] + a.q1[3]*b.q0[1] - a.q2[0]*b.q3[2] + a.q2[1]*b.q0[2] + a.q2[2]*b.q0[3] + a.q2[3]*b.q3[3] - a.q3[0]*b.q1[1] - a.q3[1]*b.q1[2] - a.q3[2]*b.q2[0] - a.q3[3]*b.q2[3],-a.q0[3]*b.q2[3] - a.q1[0]*b.q3[0] - a.q2[2]*b.q3[3] - a.q2[3]*b.q0[3] - a.q3[0]*b.q1[0] - a.q3[3]*b.q2[2],a.q0[2]*b.q2[3] - a.q1[0]*b.q3[1] + a.q2[1]*b.q3[3] + a.q2[3]*b.q0[2] - a.q3[1]*b.q1[0] + a.q3[3]*b.q2[1],a.q0[2]*b.q3[0] + a.q0[3]*b.q3[1] - a.q2[0]*b.q3[3] + a.q3[0]*b.q0[2] + a.q3[1]*b.q0[3] - a.q3[3]*b.q2[0]),
